<!-- views/url/list.hbs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of URLs</title>
</head>
<body>
<h1>Home</h1>
<table>
    <thead>
    <tr>
        <th>ID</th>
        <th>Original URL</th>
        <th>Shortened URL</th>
        <th>Counter</th>
    </tr>
    </thead>
    <tbody>
    {{#each allUrls}}
        <tr>
            <td>{{url_id}}</td>
            <td>{{url_original}}</td>
            <td><a href="localhost:3000/url/{{url_shortened}}">localhost:3000/url/{{url_shortened}}</a></td>
            <td>{{counter}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
</body>
</html>
